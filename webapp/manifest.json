{
	"_version": "1.12.0",
	"sap.app": {
		"id": "sap.ui.demo.todo",
		"i18n": "i18n/i18n.properties",
		"title": "{{TITLE}}",
		"type": "application",
		"applicationVersion": {
			"version": "0.1.0"
		}
	},
	"sap.ui": {
		"technology": "UI5",
		"deviceTypes": {
			"desktop": true,
			"tablet": false,
			"phone": false
		}
	},
	"sap.ui5": {
		"dependencies": {
			"minUI5Version": "1.75.0",
			"libs": {
				"sap.ui.core": {},
				"sap.m": {},
				"sap.f": {}
			}
		},
		"rootView": {
			"viewName": "sap.ui.demo.todo.view.App",
			"type": "XML",
			"async": true,
			"id": "app"
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "sap.ui.demo.todo.i18n.i18n",
					"supportedLocales": ["en", "de", ""],
					"fallbackLocale": "en"
				}
			},
			"": {
				"preload": true,
				"type": "sap.ui.model.json.JSONModel",
				"uri": "model/todoitems.json"
			}
		},
		"resources": {
			"css": [
				{
					"uri": "css/styles.css"
				}
			]
		},
		"contentDensities": {
		  "compact": true,
		  "cozy": false
		}
	}
}
